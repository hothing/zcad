          <Command Caption="Двигатель" Command="Insert(DEVICE_EL_MOTOR)"/> 
          <Command Caption="Распредустройство" Command="Insert(DEVICE_EL_SR)"/> 
        </SubMenu>
      </SubMenu>
      <SubMenu Name="Lighting">
